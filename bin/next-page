#!/bin/bash
umask 0077
set -euo pipefail

OUTPUT=${1:-/dev/stdout}

cat > "$OUTPUT" <<EOF
import { NextPage } from 'next'

interface Props {

}

const Page: NextPage<Props> = props => {
  return <div>Welcome to Next.js!</div>
}

Page.getInitialProps = async ctx => {
  
}

export default Page
EOF

if test -f "$OUTPUT" && type code >/dev/null >&1 ; then
  code "$OUTPUT"
fi