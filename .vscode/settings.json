{
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[fish]": {
    "editor.formatOnSave": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "gitlens.codeLens.symbolScopes": ["!Module"]
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": false,
  "debug.allowBreakpointsEverywhere": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "diffEditor.wordWrap": "off",
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.inlayHints.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.snippetSuggestions": "none",
  "editor.suggest.preview": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "**/*.json": "jsonc",
    "**/package.json": "json",
    "*.env": "shellscript",
    "*.hcl": "terraform",
    "*.lsrules": "json",
    ".env": "shellscript",
    ".env.*": "shellscript",
    "PROJECT": "yaml"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/.DS_Store": false,
    "**/.git": false,
    "**/.hg": false,
    "**/.svn": false,
    "**/CVS": false,
    "**/Thumbs.db": false
  },
  "files.insertFinalNewline": true,
  "git-graph.openNewTabEditorGroup": "Beside",
  "git-graph.repository.commits.showSignatureStatus": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.confirmSync": false,
  "github.copilot.enable": {
    "*": true,
    "markdown": false,
    "plaintext": false,
    "yaml": true
  },
  "githubPullRequests.fileListLayout": "tree",
  "gitlens.views.branches.branches.layout": "list",
  "gitlens.views.repositories.branches.layout": "list",
  "gitlens.views.repositories.files.layout": "list",
  "go.autocompleteUnimportedPackages": true,
  "go.lintFlags": ["--fast"],
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "gopls": {
    "allExperiments": true
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "notebook.lineNumbers": "on",
  "prettier.arrowParens": "avoid",
  "prettier.jsxSingleQuote": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "python.formatting.provider": "yapf",
  "python.formatting.yapfArgs": [
    "--style",
    "{indent_width: 2, continuation_indent_width: 2}"
  ],
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.pylintArgs": ["--load-plugins", "pylint_quotes"],
  "redhat.telemetry.enabled": false,
  "remote.containers.defaultExtensions": ["mutantdino.resourcemonitor"],
  "remote.containers.workspaceMountConsistency": "delegated",
  "remote.downloadExtensionsLocally": true,
  "resmon.show.cputemp": true,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "shellcheck.ignorePatterns": {
    "**/*.zlogin": true,
    "**/*.zlogout": true,
    "**/*.zprofile": true,
    "**/*.zsh": true,
    "**/*.zsh-theme": true,
    "**/*.zshenv": true,
    "**/*.zshrc": true,
    "**/.env*": true,
    "**/zlogin": true,
    "**/zlogout": true,
    "**/zprofile": true,
    "**/zshenv": true,
    "**/zshrc": true
  },
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.tabs.enabled": false,
  "terminal.integrated.tabs.hideCondition": "singleGroup",
  "terminal.integrated.tabs.showActions": "never",
  "terminal.integrated.tabs.showActiveTerminal": "never",
  "vs-kubernetes": {
    "disable-linters": ["resource-limits"]
  },
  "workbench.colorCustomizations": {
    "editor.findMatchBackground": "#ffc400",
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f",
    "terminal.selectionBackground": "#e26cffcb"
  },
  "workbench.colorTheme": "GitHub Dark",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editor.wrapTabs": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.iconTheme": "vscode-great-icons",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.settings.openDefaultKeybindings": true,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "yaml.format.singleQuote": true,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/kustomization.json": [
      "kustomization.yaml"
    ],
    "kubernetes": ["/dev/*"]
  },
  "editor.linkedEditing": true
}
