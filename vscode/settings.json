{
  "[css][graphql][html][javascript][javascriptreact][json][jsonc][markdown][mdx][scss][typescript][typescriptreact][yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[fish]": {
    "editor.formatOnSave": false
  },
  "[helm]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "gitlens.codeLens.symbolScopes": ["!Module"]
  },
  "blockman.n01LineHeight": 0,
  "blockman.n04Sub01ColorComboPresetForDarkTheme": "Classic Dark 1 (Solid colors)",
  "blockman.n04Sub05MinDistanceBetweenRightSideEdges": 8,
  "blockman.n33A01B1FromDepth0ToInwardForAllBorders": "!30,1,0,2; neutral > linear-gradient(to left, rgb(144,54,8), rgb(48,36,112))",
  "blockman.n33A01B2FromDepth0ToInwardForAllBackgrounds": "45,0,0,2; none",
  "C_Cpp.intelliSenseEngine": "disabled",
  "commit-message-editor.confirmAmend": false,
  "commit-message-editor.dynamicTemplate": [
    "{type}{scope}{breaking_change}{description}",
    "{body}",
    "{footer}"
  ],
  "commit-message-editor.staticTemplate": ["chore: "],
  "commit-message-editor.tokens": [
    {
      "description": "Increase major version  on next release - https://semver.org",
      "label": "Breaking change (required)",
      "name": "breaking_change",
      "type": "boolean"
    },
    {
      "description": "The semantic type of the changes - https://www.conventionalcommits.org",
      "label": "Type (required)",
      "multiple": true,
      "name": "type",
      "options": [
        {
          "description": "Cleaning/Refactoring/Styling/Unused commit: onDeploy => patch version (X.Y.Z+1)",
          "label": "chore"
        },
        {
          "description": "A new feature: onDeploy => if breaking change, major version (X+1.0.0), else minor version (X.Y+1.0)",
          "label": "feat"
        },
        {
          "description": "A bug fix: onDeploy => if breaking change, major version (X+1.0.0), else patch version (X.Y.Z+1)",
          "label": "fix"
        },
        {
          "description": "Changes to the build system(s): onDeploy => if breaking change, major version (X+1.0.0), else patch version (X.Y.Z+1)",
          "label": "build"
        },
        {
          "description": "Changes to the CI/CD: onDeploy => if breaking change, major version (X+1.0.0), else patch version (X.Y.Z+1)",
          "label": "ci/cd"
        },
        {
          "description": "Changes to the infrastructure",
          "label": "infra"
        },
        {
          "description": "Changes to the documentation(s): onDeploy => patch version (X.Y.Z+1)",
          "label": "docs"
        },
        {
          "description": "Changes to the test(s): onDeploy => patch version (X.Y.Z+1)",
          "label": "test"
        },
        {
          "description": "Improve production performances without a breaking change: onDeploy => patch version (X.Y.Z+1)",
          "label": "perf"
        },
        {
          "description": "Reverts a previous commit: onDeploy => if breaking change, major version (X+1.0.0), else patch version (X.Y.Z+1)",
          "label": "revert"
        }
      ],
      "separator": ",",
      "type": "enum"
    },
    {
      "description": "A noun describing a section of the codebase - https://www.conventionalcommits.org",
      "label": "Scopes",
      "name": "scope",
      "prefix": "(",
      "suffix": ")",
      "type": "text"
    },
    {
      "description": "A short summary of the code changes - https://www.conventionalcommits.org",
      "label": "Description (required)",
      "name": "description",
      "prefix": ": ",
      "type": "text"
    },
    {
      "description": "Describe the code changes - https://developers.google.com/tech-writing/overview",
      "label": "Body",
      "lines": 5,
      "multiline": true,
      "name": "body",
      "prefix": "\n",
      "type": "text"
    },
    {
      "description": "Optional footer - https://git-scm.com/docs/git-interpret-trailers",
      "label": "Footer",
      "lines": 5,
      "multiline": true,
      "name": "footer",
      "prefix": "\n",
      "type": "text"
    }
  ],
  "commit-message-editor.view.defaultView": "form",
